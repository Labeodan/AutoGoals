{
    "version": 2,
    "builds": [
      {
        "src": "script.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/",
        "dest": "script.py"
      }
    ],
    "crons": [
      {
        "path": "/cron",
        "schedule": "0 6 * * 1"
      }
    ]
  }
  